{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "jsx": "react",
      "baseUrl": "./",
      "paths": {
          "*": ["src/*"]
      }
  },
  "include": ["src", "packages/src"]
}

